#include<stdio.h>

int main(){
    int a, b;
    scanf("%d %d", &a, &b);
    for (int i=0; i<b; i++){
        if (i == 0 || i == b-1){
            for (int j=0; j<a; j++){
                printf("*");
            }
            printf("\n");
        }
        else{
            printf("*");
            for (int j=0; j<(a-2); j++){
                printf(" ");
            }
            printf("*");
            printf("\n");
        }
    }
    return 0;
}