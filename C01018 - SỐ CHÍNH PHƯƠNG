#include<stdio.h>
#include<math.h>


int main() {
    int n;
    scanf("%d", &n);
    for (int i=0; i<n; i++){
        long long temp;
        scanf("%lld", &temp);
        double sqrt_temp = sqrt(temp);
        if (sqrt_temp == (long long)sqrt_temp){
            printf("YES\n");
        }
        else{
            printf("NO\n");
        }
    }
    return 0;
}