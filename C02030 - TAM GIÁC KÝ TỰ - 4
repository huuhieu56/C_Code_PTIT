#include<stdio.h>
#include<math.h>
#include<string.h>

typedef long long ll;


int main () {
    int n;
    scanf("%d", &n);
    for (int i=0; i<n; i++) {
        for (int j = 0; j<=i*2; j+=2) {
            printf("%c", '@'+j);
        }
        for (int j=i*2-2; j>=0; j-=2) {
            printf("%c", '@'+j);
        }
        printf("\n");
    }
    return 0;
}